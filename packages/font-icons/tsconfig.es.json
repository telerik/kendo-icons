{
    "compilerOptions": {
        "outDir": "dist/es",
        "target": "es5",
        "module": "es2015",
        "moduleResolution": "node",
        "lib": ["es2015", "dom"],
        "types": [],

        "declaration": false,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "importHelpers": true,
        "noImplicitAny": false,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "removeComments": false
    },
    "files": [
        "index.ts"
    ],
    "include": [],
    "exclude": [
        "node_modules"
    ]
}
