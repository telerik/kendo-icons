{
  "name": "@progress/kendo-svg-icons",
  "description": "",
  "version": "1.0.0",
  "author": "Progress",
  "repository": "telerik/kendo-icons",
  "bugs": "https://github.com/telerik/kendo-icons/issues",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "node scripts/prepare-svg.js && node scripts/prepare-nuget.js",
    "build": "rollup -c",
    "nuget-pack": "jq '.version' package.json | xargs nuget pack src-cs/Telerik.SvgIcons/Telerik.SvgIcons.csproj -Version",
    "nuget-push": "nuget push *.nupkg -ApiKey $NUGET_API_KEY -Source $NUGET_FEED -SkipDuplicate",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "Kendo UI",
    "SVG",
    "Icons"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "rollup": "^2.23.1",
    "rollup-plugin-typescript2": "^0.27.2",
    "typescript": "^3.9.6"
  }
}
